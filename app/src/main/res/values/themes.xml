        <item name="colorPrimary">#123D5F</item>
        <item name="colorPrimaryVariant">#123D5F</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#A7B6B5</item>
